{
  "name": "kata-platform-docs",
  "description": "Documentation website for the Kata Platform.",
  "version": "1.0.0",
  "private": true,
  "author": "Resi Respati <resi@kata.ai>",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "clean": "rimraf public",
    "commit": "git cz",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "dev": "gatsby develop",
    "format": "npm run format:src && npm run format:docs",
    "format:src": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:docs": "prettier --write \"docs/**/*.md\"",
    "lint": "tslint 'src/**/*.{ts,tsx}' -p tsconfig.json",
    "start": "serve -s public",
    "test": "npm run type-check && npm run lint",
    "type-check": "tsc"
  },
  "dependencies": {
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-styled-components": "^1.5.1",
    "classnames": "^2.2.5",
    "dotenv": "^6.0.0",
    "gatsby": "^2.0.8",
    "gatsby-plugin-canonical-urls": "^2.0.5",
    "gatsby-plugin-catch-links": "^2.0.3",
    "gatsby-plugin-google-analytics": "^2.0.6",
    "gatsby-plugin-layout": "^1.0.2",
    "gatsby-plugin-manifest": "^2.0.3",
    "gatsby-plugin-netlify": "^2.0.0",
    "gatsby-plugin-netlify-cache": "^0.1.0",
    "gatsby-plugin-nprogress": "^2.0.5",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-resolve-src": "^1.1.4",
    "gatsby-plugin-sharp": "^2.0.5",
    "gatsby-plugin-styled-components": "^3.0.0",
    "gatsby-plugin-typescript": "^2.0.0",
    "gatsby-remark-autolink-headers": "^2.0.6",
    "gatsby-remark-copy-linked-files": "^2.0.5",
    "gatsby-remark-images": "^2.0.2",
    "gatsby-remark-prismjs": "^3.0.0",
    "gatsby-remark-responsive-iframe": "^2.0.5",
    "gatsby-remark-smartypants": "^2.0.5",
    "gatsby-source-filesystem": "^2.0.1",
    "gatsby-transformer-json": "^2.1.1",
    "gatsby-transformer-remark": "^2.1.3",
    "gatsby-transformer-sharp": "^2.1.2",
    "moment": "^2.22.2",
    "polished": "^2.1.1",
    "prism-themes": "^1.0.1",
    "prismjs": "^1.15.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "react-media-match": "^1.3.1",
    "styled-components": "^3.3.3",
    "typescript": "^3.0.3"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.4",
    "@types/history": "^4.6.2",
    "@types/node": "^10.5.2",
    "@types/reach__router": "^1.0.1",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "@types/react-helmet": "^5.0.6",
    "commitizen": "^2.10.1",
    "cz-conventional-changelog": "^2.1.0",
    "gh-pages": "^2.0.0",
    "prettier": "^1.13.7",
    "rimraf": "^2.6.2",
    "serve": "^10.0.2",
    "tslint": "^5.10.0",
    "tslint-config-kata": "^1.0.5",
    "tslint-react": "^3.6.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
